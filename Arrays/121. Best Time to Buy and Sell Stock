class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int low = 0;
        int max_profit = 0;
        for (int i = 0; i < prices.size(); ++i) {
            if (prices[i] < prices[low]) {
                low = i;
                continue;
            }
            max_profit= max(max_profit, prices[i] - prices[low]);
        }
        return max_profit;
        
    }
};
